{
	"Create MultiViz connector plugin": {
		"prefix": "mv-connector",
		"body": [
			"{",
			"    \"$schema\": \"https://vikinganalytics.github.io/multiviz/extension-schema.json\",",
			"    \"name\": \"${1:plugin name, e.g. 'MultiViz connector plugin for ...'}\",",
			"    \"description\": \"${2:longer description of the plugin}\",",
			"    \"extension\": \"${3:short unique plugin/extension name, e.g. my_connector_plugin}\",",
			"    \"version\": \"1.0.0\",",
			"    \"type\": \"python\",",
			"    \"run\": \"main.py\",",
			"    \"triggers\": [",
			"        \"connector\"",
			"    ],",
			"    \"parameters\": {",
			"        \"start_date\": {",
			"            \"type\": \"string\",",
			"            \"name\": \"Start date\",",
			"            \"description\": \"Enter start date (format: 'YYYY-MM-DD')\",",
			"            \"default\": \"2019-01-01\",",
			"            \"hidden\": true,",
			"        },",
			"        \"stop_date\": {",
			"            \"type\": \"string\",",
			"            \"name\": \"Stop date\",",
			"            \"description\": \"Enter stop date (format: 'YYYY-MM-DD')\",",
			"            \"default\": \"2019-10-23\",",
			"            \"hidden\": true",
			"        },",
			"        \"project_id\": {",
			"            \"type\": \"string\",",
			"            \"name\": \"Project id\",",
			"            \"description\": \"Project identifier\",",
			"            \"default\": \"\",",
			"            \"hidden\": true",
			"        },",
			"        \"task\": {",
			"            \"type\": \"string\",",
			"            \"name\": \"Task\",",
			"            \"description\": \"Task identifier, e.g. get_instrumentation, download_data, import_data\",",
			"            \"default\": \"\",",
			"            \"hidden\": true",
			"        },",
			"        \"my_string_parameter\": {",
			"            \"type\": \"string\",",
			"            \"name\": \"String parameter\",",
			"            \"description\": \"This is a sample of a string parameter\",",
			"            \"default\": \"\"",
			"        },",
			"        \"my_integer_parameter\": {",
			"            \"type\": \"integer\",",
			"            \"name\": \"Integer parameter\",",
			"            \"description\": \"This is a sample of an integer parameter\",",
			"            \"default\": 100",
			"        },",
			"        \"my_select_parameter\": {",
			"            \"type\": \"select\",",
			"            \"name\": \"Select parameter\",",
			"            \"description\": \"This is a sample of a select parameter\",",
			"            \"options\": [",
			"                {",
			"                    \"label\": \"Type 1\",",
			"                    \"value\": 0",
			"                },",
			"                {",
			"                    \"label\": \"Type 2\",",
			"                    \"value\": 1",
			"                },",
			"                {",
			"                    \"label\": \"Type 3\",",
			"                    \"value\": 2",
			"                }",
			"            ]",
			"        }",
			"    }",
			"}"
		],
		"description": "Create MultiViz connector plugin template"
	},
